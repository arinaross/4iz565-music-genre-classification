       "Track Name              object\n",
       "Popularity             float64\n",
       "danceability           float64\n",
       "energy                 float64\n",
       "key                   category\n",
       "loudness               float64\n",
       "mode                  category\n",
       "speechiness            float64\n",
       "acousticness           float64\n",
       "instrumentalness       float64\n",
